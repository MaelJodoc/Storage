CREATE TABLE ITEM
(
  ID bigint AUTO_INCREMENT PRIMARY KEY NOT NULL,
  CUSTOM_CODE varchar(50),
  DESCRIPTION varchar(255),
  MANUFACTURER varchar(50),
  MANY_UNITS integer,
  MEDIUM_UNITS integer,
  MIN_UNITS integer,
  MMK_CODE varchar(50),
  NAME varchar(100),
  QUANTITY_PER_UNIT integer,
  UNIT varchar(20)
)